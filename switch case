// Java Program to check the size
// using the switch...case statement
class Main {
  public static void main(String[] args) {
    int number = 3;
    String size;
    // switch statement to check size
    switch (string) 
    {
      case 1:
        size = "Small";
        break;
      case 2:
        size = "Medium";
        break;
      // match the value of week
      case 3:
        size = "Large";
        break;
      case 4:
        size = "Extra Large";
        break;
      default:
        size = "Unknown";
        break; 
}
    
    System.out.println("Size: " + size);
  }
}
